<% f.inputs do %>
  <%= f.input :asset_date_type_name, :as => :pbcore_combobox,
                                     :input_html => { :class => "pbcore-autocomplete", 
                                                      "data-autocomplete-source" => asset_date_types_path },
                                     :wrapper_html => { :class => "col-1" },
                                     :label => "Date Type",
                                     :required => false %>
  <%= f.input :asset_date, :as => :string, 
                           :label => "Date", 
                           :required => false,
                           :wrapper_html => { :class => "col-2" } %>
  <%= link_to_remove_fields "remove", f %>
<% end %>